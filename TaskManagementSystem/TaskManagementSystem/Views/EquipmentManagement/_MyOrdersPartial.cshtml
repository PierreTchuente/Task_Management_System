<table class="table table-hover table-striped table-bordered table-responsive" ng-cloak>
    <tr>
        <th class="span1"></th>
        <th class="span2"><b>Equipment</b></th>
        <th class="span3"><b>Quantity</b></th>
        <th class="span4"><b>Status</b></th>
        <th class="span5"><b>Time Out</b></th>
        <th class="span6"><b>Time Returned</b></th>
        <th class="span7"><b>Delete Order</b></th>
    </tr>
    <tr ng-repeat="(key, cartItem) in shoppingCart">
        <td style="color:black">{{ $index + 1 }}</td>
        <td>{{ cartItem.EQ_Name }}</td>
        <td>{{ cartItem.OE_QuantityOrdered }}</td>
        <td class="{{getStatusColor(cartItem.STATUST_ID)}}">{{ cartItem.STATUST_Desc }}</td>
        <td>{{ cartItem.OE_Out_Time }}</td>
        <td>{{ cartItem.OE_Return_Time }}</td>
        @*<td><span class="glyphicon glyphicon-remove-circle" aria-hidden="true" data-toggle="modal" data-target="#modalCheckDeleteOrderEquipmentManagement" ng-click="setParamsForDelete(cartItem.OE_ID, cartItem.EQ_Name)" ng-show="cartItem.statusCheck"></span></td>*@
        <td><span style="color:black;align-items:center" class="glyphicon glyphicon-remove-circle" aria-hidden="true" data-toggle="modal" data-target="#modalCheckDeleteOrderEquipmentManagement" ng-click="setParamsForDeleteEQPage(cartItem.OE_ID, cartItem.EQ_Name)"></span></td>
     </tr>

</table>




<div class="modal fade" id="modalCheckDeleteOrderEquipmentManagement" role="dialog">
    <div class="modal-dialog modal-sm">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title due-date-sticker red-sticker">WARNING!</h4>
            </div>
            <div class="modal-body">
                <p>Are you sure you want to delete the order?</p>
            </div>
            <div class="modal-footer">
                <button ng-click="deleteOrderEQPage()" type="button" class="btn btn-default" data-dismiss="modal">Delete Order</button>
            </div>
        </div>
    </div>
</div>
