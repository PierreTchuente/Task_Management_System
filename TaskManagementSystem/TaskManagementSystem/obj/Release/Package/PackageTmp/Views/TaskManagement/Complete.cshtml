                <div class="form-group">
                    <label class="title-content" for="form_message"><i class="fa fa-list" aria-hidden="true"></i> <span style="margin-left:3px"><b>Description: {{model.CompletedDescription}}</b> </span></label>
                    <p></p>
                    <div class="help-block with-errors"></div>

                    <div class="col-md-12 row">

                    </div>

                </div>

                <hr />
                <div class="form-group">
                    <p><i class="fa fa-calendar" aria-hidden="true" style="margin-right:3px"></i>  Start Date<b>:   {{model.TASK_ExpectedStartDate}}</b> </p>
                    <p><i class="fa fa-calendar" aria-hidden="true" style="margin-right:3px"></i>  Expected End Date<b>:   {{model.TASK_ExpectedEndDate}}</b></p>
                    <p><i class="fa fa-calendar" aria-hidden="true" style="margin-right:3px"></i>  Actual End Date<b>:   {{model.TASK_ActualEndDate}}</b></p>
                </div>
                <div class="col-md-12 row">

                </div>


                <div class="col-md-12 row add-check">
                    <label class="title-content">
